# Google Gemini API Keys (not a must to be up to 10 keys)
GENAI_API_KEY_1=your_first_api_key_here
GENAI_API_KEY_2=your_second_api_key_here
GENAI_API_KEY_3=your_third_api_key_here
GENAI_API_KEY_4=your_fourth_api_key_here
GENAI_API_KEY_5=your_fifth_api_key_here
GENAI_API_KEY_6=your_sixth_api_key_here
GENAI_API_KEY_7=your_seventh_api_key_here
GENAI_API_KEY_8=your_eighth_api_key_here
GENAI_API_KEY_9=your_ninth_api_key_here
GENAI_API_KEY_10=your_tenth_api_key_here

# Server Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false
LOG_LEVEL=INFO

# CORS Configuration (comma-separated domains)
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# File Storage
UPLOADS_DIR=uploads
MAX_FILE_SIZE_MB=50
CLEANUP_OLD_FILES_DAYS=7

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=60
RATE_LIMIT_REQUESTS_PER_HOUR=1000

# Retry Configuration
MAX_RETRIES=3
RETRY_DELAY_SECONDS=5
API_KEY_ROTATION_ENABLED=true

# Security
SECRET_KEY=your-secret-key-here-change-this-in-production 